{

	public PokerHand(String _playerName)
	{
		super(_playerName);
	}
	
	@Override
	protected void calculateHandValue()
	{
		for (PokerCard card : this.cards)
		{
			this.handValue = card.cardValue;
		}
	}

	@Override
	public void removeCard(PokerCard card)
	{
		
		
	}
	
}
